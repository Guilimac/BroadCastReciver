<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fiap.broadcastreceiver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".reciver.MyReciver">
            <intent-filter>
                <action android:name="android.app.action.ACTION_PASSWORD_SUCCEEDED">

                </action>
            </intent-filter>
        </receiver>

        <receiver android:name=".reciver.MyOrderedReciver">
            <intent-filter android:priority="1">
                <action android:name="orderedBroadcast">

                </action>
            </intent-filter>
        </receiver>

        <receiver android:name=".reciver.MySecondReciver">
            <intent-filter android:priority="2">
                <action android:name="orderedBroadcast2">
                    
                </action>
            </intent-filter>
        </receiver>
    </application>

</manifest>